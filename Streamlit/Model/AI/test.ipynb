    "from gtts import gTTS \n",
    "from mpyg321.MPyg123Player import MPyg123Player\n",
    "mytext = 'thank you'\n",
    "language = 'en'\n",
    "myobj = gTTS(text=mytext, lang=language, slow=False) \n",
    "myobj.save(\"welcome.mp3\") \n",
    "player = MPyg123Player()\n",
    "player.play_song(\"welcome.mp3\")\n"
